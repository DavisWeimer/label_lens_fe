
<h1>Create an Account</h1>

<%= form_with(model: @user, url: 'http://localhost:3000/login_email', method: 'post') do |f| %>
  <div class="field">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>

  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  
  </div>

  <div class="actions">
    <%= f.submit 'Create Account' %>
  </div>
<% end %>
